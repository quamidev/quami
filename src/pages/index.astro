---
import Layout from '../layouts/Layout.astro';
import Logo from '../components/Logo.astro';
---

<Layout title="Quami - Software Studio">
  <!-- Navigation -->
  <nav>
    <div class="nav-container">
      <a href="#" class="nav-logo">
        <Logo />
